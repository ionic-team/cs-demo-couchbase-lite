{
  "extends": "../tslint.json",
  "linterOptions": {
    "exclude": [
      "polyfills.ts"
    ]
  },
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "page",
      "kebab-case"
    ]
  }
}
